{
  "extends": "airbnb-base",
  "root": true,
	"env": {
    "node": true,
    "jest": true
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
   	"comma-dangle": [
      "error",
      "never"
    ],
    "no-debugger": 1,
    "no-underscore-dangle": 0,
    "object-curly-spacing": 0,
    "object-curly-newline": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  }
}